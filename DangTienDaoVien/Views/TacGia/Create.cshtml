@model DangTienDaoVien.Models.TacGia

<div class="mt-5"> 
	<form action="Create" method="post">
		<div class="form-group my-2">
			<label asp-for="Ten"></label>
			<input asp-for="Ten" class="form-control" />
			<span asp-validation-for="Ten" class="text-danger"></span>
			<hr />
			<label asp-for="MoTa"></label>
			<input asp-for="MoTa" class="form-control" />
			<span asp-validation-for="MoTa" class="text-danger"></span>
		</div>
		<button type="submit" class="btn btn-primary"><h4>Tạo</h4></button>
	</form>
</div>
@section Scripts {
	<partial name="_ValidationScriptsPartial" />
}